<Bindings>
	<Binding name="FISHING_MODE_TOGGLE" category="Fishing Mode">
        FishingMode:Toggle()
	</Binding>
	<Binding name="FISHING_MODE_ON" category="Fishing Mode">
        FishingMode:Start()
	</Binding>
	<Binding name="FISHING_MODE_OFF" category="Fishing Mode">
        FishingMode:Stop()
	</Binding>
</Bindings>